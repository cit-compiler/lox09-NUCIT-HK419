expression  ::= literal
            | unary
            | binary
            | grouping

literal     ::= NUMBER | STRING | "true" | "false" | "nil
grouping    ::= "(" expression ")"
unary       ::= ( "-" | "!" ) expression
binary      ::= expression operetor expression
operator    ::= "==" | "!=" | "<" | ">" | "<=" | ">=" | "+" | "-" | "*" | "/"